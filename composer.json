{
  "name": "mr2p/query-loop-enhancements",
  "type": "wordpress-plugin",
  "description": "Make everyone love the Query Loop block.",
  "keywords": [
    "WordPress",
    "Plugins"
  ],
  "homepage": "https://github.com/mr2p/query-loop-enhancements",
  "license": "GPL-3.0-or-later",
  "authors": [],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^1.1",
    "php-parallel-lint/php-parallel-lint": "^1.3.1",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "squizlabs/php_codesniffer": "^3.13",
    "wp-coding-standards/wpcs": "^3.2"
  },
  "scripts": {
    "lint:wpcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
    "lint:php": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor ."
  },
  "support": {
    "issues": "https://github.com/mr2p/query-loop-enhancements/issues",
    "source": "https://github.com/mr2p/query-loop-enhancements"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
